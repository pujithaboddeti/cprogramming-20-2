#include <stdio.h>
int main()
{

    int n,ele,i,h;
    printf("enter n value");
    scanf("%d",&n);
    i=1;
    while(i<=n)
    {
        printf("\nenter element");
        scanf("%d",&ele);
        if(i==2)
        {
            h=ele;
            if(ele<h)
            {
                h=ele;
            }
        }
        i++;
    }
    printf("\n highest %d",h);
}
